params {
    config_profile_name        = 'Test profile'
    config_profile_description = 'Minimal test dataset to check pipeline function'
    
    // genome
    genome = "GRCh38"
    
    // parabricks
    known_sites = ["s3://elembio-hosting-d-data/gatk/Homo_sapiens_assembly38/1000G_phase1.snps.high_confidence.hg38.vcf.gz","s3://elembio-hosting-d-data/gatk/Homo_sapiens_assembly38/Mills_and_1000G_gold_standard.indels.hg38.vcf.gz","s3://elembio-hosting-d-data/gatk/Homo_sapiens_assembly38/dbsnp_144.hg38.vcf.gz"]

    // input
    input = "${baseDir}/tests/csv/small_fastq_hosting.csv"

    // output
}

cleanup = false

aws {
   batch {
      cliPath = '/home/ec2-user/miniconda/bin/aws'
      maxParallelTransfers = 10
      maxTransferAttempts = 3
   }
   client {
      maxConnections = 10
      uploadMaxThreads = 10
      maxErrorRetry = 3
      socketTimeout = 3600000
      uploadRetrySleep = 1000
      uploadChunkSize = 32.MB
   }
}
